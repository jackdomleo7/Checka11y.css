{
  "name": "checka11y-css-docs",
  "private": true,
  "version": "1.0.0",
  "description": "Documentation website for Checka11y.css.",
  "main": "index.js",
  "scripts": {
    "docs": "stylelint \"**/*.scss\" --fix && node-sass scss/docs.scss css/docs.min.css && node-sass scss/checka11y.scss css/checka11y.min.css && node-sass scss/prism.scss css/prism.min.css && npx postcss css --use autoprefixer cssnano --replace && npx sitemap-generator-cli https://checka11y.jackdomleo.dev --last-mod",
    "serve": "http-server ./ --port 1234 -a localhost"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackdomleo7/Checka11y.css.git"
  },
  "author": {
    "name": "Jack Domleo",
    "url": "https://jackdomleo.dev"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackdomleo7/Checka11y.css/issues"
  },
  "dependencies": {
    "normalize.css": "^8.0.1"
  },
  "homepage": "https://checka11y.jackdomleo.dev",
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "checka11y-css": "^1.3.3",
    "cssnano": "^4.1.10",
    "http-server": "^0.12.3",
    "node-sass": "^5.0.0",
    "postcss-cli": "^8.3.1",
    "postcss": "^8.2.8",
    "sitemap-generator-cli": "^7.5.0",
    "stylelint": "^13.12.0",
    "stylelint-scss": "^3.19.0"
  }
}
